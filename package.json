{
  "name": "ide-html",
  "version": "0.1.0",
  "description": "Atom-IDE support for HTML language",
  "main": "src/main.js",
  "scripts": {
    "eslint": "eslint ./src",
    "test": "npm run eslint"
  },
  "repository": "https://github.com/liuderchi/ide-html",
  "keywords": [
    "atom-ide",
    "html",
    "html-parsing",
    "ide",
    "language-server-protocol",
    "outline-view"
  ],
  "author": "Te-Chi Liu",
  "license": "MIT",
  "engines": {
    "atom": ">=1.21.0"
  },
  "bugs": {
    "url": "https://github.com/liuderchi/ide-html/issues"
  },
  "homepage": "https://github.com/liuderchi/ide-html#readme",
  "devDependencies": {
    "eslint": "^4.7.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.6.3",
    "fuzzaldrin-plus": "^0.5.0",
    "vscode-html-languageserver-bin": "^1.0.0"
  },
  "enhancedScopes": [
    "text.html.basic"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  }
}
